{
  "name": "anomix-network",
  "private": true,
  "version": "0.0.0",
  "description": "",
  "author": "",
  "license": "MIT",
  "workspaces": [
    "packages/*", 
    "apps/*"
  ],
  "scripts": {
    "install": "npm run libs:build",
    "apps:dev": "concurrently \"npm run serve -w ano-cash-ui\" \"npm run serve -w anomix-rollup-sequencer\"",
    "libs:dev": "concurrently \"npm run lib:dev -w @anomix/utils\" \"npm run lib:dev -w @anomix/shared\"",
    "web:dev": "npm run serve -w ano-cash-ui",
    "server:dev": "npm run serve -w anomix-rollup-sequencer",
    "server:cli": "npm run cli -w anomix-rollup-sequencer",
    "libs:build": "npm run lib:build --workspaces --if-present",
    "build": "npm run lib:build --workspaces --if-present && npm run build --workspaces --if-present",
    "clean": "rimraf ./_dist && npm run lib:clean --workspaces --if-present",
    "lint": "npm run lint --workspaces --if-present && npm run lib:lint --workspaces --if-present"
  },
  "dependencies": {
    "axios": "^0.27.2",
    "shelljs": "^0.8.5",
    "snarkyjs": "0.12.*"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/cron": "^2.0.0",
    "@types/express": "^4.17.13",
    "@types/supertest": "^2.0.11",
    "source-map-support": "^0.5.20",
    "@intlify/vite-plugin-vue-i18n": "^3.4.0",
    "@quasar/vite-plugin": "^1.0.9",
    "@vitejs/plugin-vue": "^2.3.1",
    "@vue/tsconfig": "^0.1.3",
    "prettier": "^2.5.1",
    "sass": "1.32.12",
    "vite": "^2.9.9",
    "vite-plugin-checker": "^0.4.6",
    "vue-tsc": "^0.34.7",
    "supertest": "^6.1.3",
    "@types/estree": "^0.0.51",
    "@rushstack/eslint-patch": "^1.1.0",
    "@types/node": "^16.11.27",
    "@typescript-eslint/eslint-plugin": "^5.21.0",
    "@typescript-eslint/parser": "^5.21.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "concurrently": "^7.2.0",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.2.0",
    "pkg": "^5.6.0",
    "rimraf": "3.0.2",
    "copyfiles": "2.4.1",
    "ts-jest": "^27.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "tslib": "^2.4.0",
    "typescript": "^4.9.5"
  }
}
