{
  "name": "@anomix/circuits",
  "version": "0.1.0",
  "description": "",
  "author": "",
  "license": "Apache-2.0",
  "keywords": [
    "mina-zkapp",
    "mina-zk-app",
    "mina-dapp",
    "zkapp"
  ],
  "types": "dist/src/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist_es/src/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "scripts": {
    "ok": "tsc -p tsconfig.json && node --experimental-specifier-resolution=node ./build/src/ok.js",
    "demo": "tsc -p tsconfig.json && node --experimental-specifier-resolution=node ./build/src/demo.js",
    "lib:dev": "concurrently \"tsc --build tsconfig.json --watch\" \"tsc-alias -w\" \"tsc --build tsconfig.es.json --watch\" \"tsc-alias -w -p tsconfig.es.json\"",
    "lib:build": "concurrently \"tsc --build tsconfig.json\" \"tsc-alias\" \"tsc --build tsconfig.es.json\" \"tsc-alias -p tsconfig.es.json\"",
    "lib:clean": "rimraf ./dist ./dist_es ./tsconfig.es.tsbuildinfo ./tsconfig.tsbuildinfo",
    "lib:lint": "eslint \"src/**/*.ts\" --fix",
    "build": "npm run lib:build",
    "buildw": "npm run lib:dev",
    "coverage": "node --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --coverage",
    "format": "prettier --write --ignore-unknown **/*",
    "test": "node --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js",
    "testw": "node --experimental-vm-modules --experimental-wasm-threads node_modules/jest/bin/jest.js --watch"
  },
  "lint-staged": {
    "**/*": [
      "eslint src/* --fix",
      "prettier --write --ignore-unknown"
    ]
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "eslint": "^8.7.0",
    "eslint-plugin-snarkyjs": "^0.1.0",
    "husky": "^7.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^11.0.1",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "snarkyjs": "0.12.*"
  },
  "dependencies": {
    "@anomix/merkle-tree": "^0.1.0"
  }
}