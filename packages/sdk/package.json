    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "snarkyjs": "0.12.*"
  },
  "dependencies": {
    "@anomix/merkle-tree": "^0.1.0",
