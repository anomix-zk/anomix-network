{
  "name": "@anomix/sdk",
  "private": true,
  "description": "sdk within Anomix-Network",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "scripts": {
    "sdk": "tsc --build tsconfig.json && node --stack-trace-limit=1000 --experimental-specifier-resolution=node ./dist/src/sdk_test.js",
    "node": "tsc --build tsconfig.json && node --experimental-specifier-resolution=node ./dist/src/rollup_node/node_Provider_test.js",
    "demo": "tsc --build tsconfig.json && node --experimental-specifier-resolution=node ./dist/src/demo_test.js",
    "lib:dev": "npm run lib:build && (concurrently \"tsc --build tsconfig.json --watch\" \"tsc-alias -w\")",
    "lib:build": "tsc --build tsconfig.json && tsc-alias",
    "lib:clean": "rimraf ./dist ./dist_es ./tsconfig.es.tsbuildinfo ./tsconfig.tsbuildinfo",
    "lib:lint": "eslint \"src/**/*.ts\" --fix"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-typescript": "^7.16.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@types/jest": "^27.0.3",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "axios": "^1.4.0",
    "bignumber.js": "^9.1.1",
    "fake-indexeddb": "^4.0.2",
    "husky": "^7.0.1",
    "jest": "^27.3.1",
    "lint-staged": "^11.0.1",
    "mina-signer": "^2.1.0",
    "prettier": "^2.3.2",
    "ts-jest": "^27.0.7",
    "typescript": "^4.7.2"
  },
  "peerDependencies": {
    "snarkyjs": "0.12.*"
  },
  "dependencies": {
    "@anomix/circuits": "^0.1.0",
    "@anomix/merkle-tree": "^0.1.0",
    "@anomix/utils": "^0.1.0",
    "async-mutex": "^0.4.0",
    "comlink": "^4.4.1",
    "consola": "^3.2.3",
    "detect-node": "^2.1.0",
    "dexie": "^3.2.4",
    "isomorphic-fetch": "^3.0.0"
  }
}
