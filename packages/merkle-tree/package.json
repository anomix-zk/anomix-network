{
  "name": "@anomix/merkle-tree",
  "private": true,
  "description": "merkle-tree structures within Anomix-Network",
  "version": "1.0.0",
  "main": "dist/index.js",
  "module": "dist_es/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist_es/index.js",
      "require": "./dist/index.js"
    }
  },
  "license": "MIT",
  "scripts": {
    "lib:dev": "concurrently \"tsc --build tsconfig.json --watch\" \"tsc --build tsconfig.es.json --watch\"",
    "lib:build": "concurrently \"tsc --build tsconfig.json\" \"tsc --build tsconfig.es.json\"",
    "lib:clean": "rimraf ./dist ./dist_es ./tsconfig.es.tsbuildinfo ./tsconfig.tsbuildinfo",
    "lib:lint": "eslint \"src/**/*.ts\" --fix"
  },
  "dependencies": {
    "levelup": "^5.1.1",
    "memdown": "^6.1.1"
  },
  "devDependencies": {
    "@types/levelup": "^5.1.2",
    "@types/memdown": "^3.0.1"
  }
}