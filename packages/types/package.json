{
  "name": "@anomix/types",
  "private": true,
  "description": "common types for Anomix-Network",
  "version": "0.1.0",
  "type": "module",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "license": "MIT",
  "scripts": {
    "lib:dev": "npm run lib:build && (concurrently \"tsc --build tsconfig.json --watch\" \"tsc-alias -w\")",
    "lib:build": "npm run gen-schema-txreqdto && npm run gen-schema-txrespdto && npm run gen-schema-mpdto && npm run gen-schema-widto && npm run gen-schema-enib && tsc --build tsconfig.json && tsc-alias",
    "lib:clean": "rimraf ./dist ./dist_es ./tsconfig.es.tsbuildinfo ./tsconfig.tsbuildinfo",
    "lib:lint": "eslint \"src/**/*.ts\" --fix",
    "gen-schema-txreqdto": "npx typescript-json-schema ./src/l2-tx-req-dto.ts L2TxReqDto -o /src/l2-tx-req-dto-schema.json",
    "gen-schema-txrespdto": "npx typescript-json-schema ./src/l2-tx-resp-dto.ts L2TxRespDto -o /src/l2-tx-resp-dto-schema.json",
    "gen-schema-mpdto": "npx typescript-json-schema ./src/merkle-proof-dto.ts MerkleProofDto -o /src/merkle-proof-dto-schema.json",
    "gen-schema-widto": "npx typescript-json-schema ./src/withdraw-info-dto.ts WithdrawInfoDto -o /src/withdraw-info-dto-schema.json",
    "gen-schema-enib": "npx typescript-json-schema ./src/encrypted-asset-in-block.ts EncryptedNoteInBlock -o /src/encrypted-asset-in-block-schema.json"
  }
}