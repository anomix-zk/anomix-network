{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "description": "used for All L2 tx scenarios",
    "properties": {
        "extraData": {
            "properties": {
                "acctPk": {
                    "description": "used at Account Registration section",
                    "type": "string"
                },
                "aliasHash": {
                    "description": "used at Account Registration section",
                    "type": "string"
                },
                "outputNote1": {
                    "properties": {
                        "cipherText": {
                            "description": "long text. from encrypt(valueNote.json())",
                            "type": "string"
                        },
                        "noteCommitment": {
                            "type": "string"
                        },
                        "publicKey": {
                            "description": "Base58",
                            "type": "string"
                        },
                        "receiverInfo": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array"
                        }
                    },
                    "type": "object"
                },
                "outputNote2": {
                    "properties": {
                        "cipherText": {
                            "description": "long text. from encrypt(valueNote.json())",
                            "type": "string"
                        },
                        "noteCommitment": {
                            "type": "string"
                        },
                        "publicKey": {
                            "description": "Base58",
                            "type": "string"
                        },
                        "receiverInfo": {
                            "items": {
                                "type": "string"
                            },
                            "type": "array"
                        }
                    },
                    "type": "object"
                },
                "withdrawNote": {
                    "description": "used at Withdrawal section",
                    "properties": {
                        "accountRequired": {
                            "type": "string"
                        },
                        "assetId": {
                            "type": "string"
                        },
                        "creatorPk": {
                            "type": "string"
                        },
                        "inputNullifier": {
                            "type": "string"
                        },
                        "noteType": {
                            "type": "string"
                        },
                        "ownerPk": {
                            "type": "string"
                        },
                        "secret": {
                            "type": "string"
                        },
                        "value": {
                            "type": "string"
                        }
                    },
                    "type": "object"
                }
            },
            "type": "object"
        },
        "proof": {
            "description": "from execution result of Join-Split circuit",
            "properties": {
                "maxProofsVerified": {
                    "enum": [
                        0,
                        1,
                        2
                    ],
                    "type": "number"
                },
                "proof": {
                    "type": "string"
                },
                "publicInput": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                },
                "publicOutput": {
                    "items": {
                        "type": "string"
                    },
                    "type": "array"
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}

