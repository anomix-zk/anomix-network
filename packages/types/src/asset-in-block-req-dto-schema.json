{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "properties": {
        "blocks": {
            "description": "block height list, no requirements on sequence.",
            "items": {
                "type": "number"
            },
            "type": "array"
        },
        "flag": {
            "type": "number"
        },
        "range": {
            "properties": {
                "end": {
                    "description": "greater than 'start'",
                    "type": "number"
                },
                "start": {
                    "description": "less than 'end'",
                    "type": "number"
                }
            },
            "type": "object"
        }
    },
    "type": "object"
}

